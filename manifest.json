{
    "name": "Assi Word Replacer",
    "version": "1.0.0",
    "description": "Replace Assi with amazing human being",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*/**"],
            "all_frames": true,
            "js": ["word_replace.js"],
            "run_at": "document_end"
        }
    ]
}
