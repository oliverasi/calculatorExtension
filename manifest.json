{
    "name": "Assi Word Replacer",
    "version": "1.0.0",
    "description": "Replace Assi with Ass",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["*://*/**"],
            "all_frames": true,
            "js": ["word_replace.js"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "index.html", //your html document
        "default_title": "Js calculator" //your html document title
    },


    "permissions": ["tabs"]
}
